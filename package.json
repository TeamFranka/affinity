{
  "name": "affinity",
  "version": "0.0.1",
  "private": true,
  "description": "Affinity Progressive Communities Network App",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e --url http://localhost:8080/",
    "build:widget": "vue-cli-service build --target wc --inline-vue --name affinity-chat ./src/components/config/Widget.vue",
    "ci:run-parse": "npm run dev:build-parse && docker-compose up -d && docker-compose logs --tail 30",
    "ci:setup-db": "npm run dev:db && npm run dev:db:mock-data && cp .env.development.local.template .env.development.local",
    "ci:lint": "vue-cli-service lint --no-fix",
    "deploy:b4a": "npm run deploy:web && npm run deploy:db",
    "deploy:db": "node -r dotenv/config bin/update-db.js dotenv_config_path=.env.production.local",
    "deploy:web": "ionic build --engine browser && cd backend/cloud && npm install && cd ../ && b4a deploy",
    "dev:android": "ionic capacitor run android -l --external",
    "dev:build-parse": "docker-compose build --no-cache parse",
    "dev:db": "node -r dotenv/config bin/update-db.js dotenv_config_path=.env.development",
    "dev:db:add-faqs": "node -r dotenv/config bin/mock-data.js dotenv_config_path=.env.development with-faq",
    "dev:db:add-posts": "node -r dotenv/config bin/mock-data.js dotenv_config_path=.env.development with-posts",
    "dev:db:destroy": "docker-compose stop && docker-compose rm -f",
    "dev:db:mock-data": "node -r dotenv/config bin/mock-data.js dotenv_config_path=.env.development with-faq with-posts",
    "dev:db:reset": "npm run dev:db:destroy && docker-compose up -d && npm run dev:db && npm run dev:db:mock-data && cp .env.development.local.template .env.development.local",
    "dev:ios": "ionic capacitor run ios -l --external",
    "dev:lint": "vue-cli-service lint --fix",
    "dev:restart-parse": "npm run dev:build-parse && docker-compose stop parse && docker-compose up -d && docker-compose logs -f --tail 10 parse",
    "dev:run-parse": "docker-compose up -d && docker-compose logs -f --tail 30 parse",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\" --output locales-report.json"
  },
  "dependencies": {
    "@capacitor/android": "^3.0.0-rc.0",
    "@capacitor/app": "^0.3.3",
    "@capacitor/camera": "^0.4.0",
    "@capacitor/core": "3.0.0-rc.0",
    "@capacitor/ios": "^3.0.0-rc.0",
    "@capacitor/push-notifications": "^0.3.3",
    "@ionic/vue": "^5.6.0",
    "@ionic/vue-router": "^5.6.0",
    "@types/markdown-it": "^12.0.1",
    "@types/parse": "^2.18.4",
    "@types/pell": "^1.0.0",
    "@types/turndown": "^5.0.0",
    "affinity-backend": "file:backend/cloud",
    "appcenter": "^4.0.2",
    "core-js": "^3.9.0",
    "dayjs": "^1.10.4",
    "easyqrcodejs": "^4.3.2",
    "get-urls": "git+https://github.com/gnunicorn/get-urls.git",
    "ionicons": "^5.5.0",
    "lodash": "^4.17.21",
    "markdown-it": "^12.0.4",
    "parse": "^3.1.0",
    "register-service-worker": "^1.7.1",
    "turndown": "^7.0.0",
    "vue": "^3.0.6",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.4",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.12.13",
    "@capacitor-community/http": "^1.0.0-alpha.1",
    "@capacitor/cli": "3.0.0-rc.0",
    "@capacitor/device": "^0.5.6",
    "@capacitor/dialog": "^0.4.6",
    "@capacitor/filesystem": "^0.5.1",
    "@intlify/vue-i18n-loader": "^2.0.0-rc.1",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-e2e-cypress": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-pwa": "^4.5.11",
    "@vue/cli-plugin-router": "^4.5.11",
    "@vue/cli-plugin-typescript": "^4.5.11",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-service": "4.5.9",
    "@vue/compiler-sfc": "^3.0.6",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-rc.1",
    "capacitor-codepush": "git+https://github.com/mapiacompany/capacitor-codepush.git",
    "cypress": "^6.5.0",
    "cypress-localstorage-commands": "^1.4.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.9",
    "vue-cli-plugin-i18n": "~2.0.1",
    "vue-jest": "^5.0.0-alpha.8",
    "webpack-chain": "^6.5.1"
  }
}
